{
  "name": "ffbleaguemanager",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build",
    "test": "npm run lint && cross-env NODE_ENV=test mocha -u tdd --reporter spec --exit",
    "lint": "eslint \"**/*.js\"",
    "fix": "eslint --fix ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rise-shine/FFBLeagueManager.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rise-shine/FFBLeagueManager/issues"
  },
  "homepage": "https://github.com/rise-shine/FFBLeagueManager#readme",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "bluebird": "^3.5.5",
    "cheerio": "^1.0.0-rc.3",
    "express": "^4.17.1",

    "mongoose": "^5.6.8",
    "morgan": "^1.9.1",
    "if-env": "^1.0.4",

    "mysql2": "^1.6.5",
    "react-switch": "^5.0.1",
    "react-toggle": "^4.0.2",
    "sequelize": "^5.12.2"
  }
}
